{
  "type": "table",
  "data": {
    "source": {
      "provider": "graph",
      "subgraphId": "messari/uniswap-v3-ethereum",
      "entity": "liquidityPoolDailySnapshots",
      "orderDirection": "desc",
      "orderBy": "dailyVolumeUSD",
      "skip": 1,
      "first": 20,
      "filters": {
        "timestamp_gte": "$startOfDay"
      }
    },
    "group": {
      "key": "pool.symbol",
      "aggregations": {
        "dailyVolumeUSD": "sum"
      }
    }
  },
  "table": {
    "columns": [
      {
        "dataKey": "pool.symbol",
        "label": "Pool"
      },
      {
        "dataKey": "dailyVolumeUSD",
        "label": "Daily Volume (USD)",
        "format": "number"
      }
    ]
  }
}
