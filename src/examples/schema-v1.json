{
  "$schema": "https://chainlook.xyz/schema/widget/1",
  "title": "Usage Trend - Last 30 Days",
  "type": "chart",
  "author": {
    "address": "",
    "name": ""
  },
  "data": {
    "sources": {
      "uniswap": {
        "provider": "graph",
        "subgraphId": "messari/uniswap-v3-ethereum",
        "entity": "usageMetricsDailySnapshots",
        "orderDirection": "desc",
        "orderBy": "timestamp",
        "skip": 0,
        "first": 20,
        "filters": {}
      },
      "saddle": {
        "provider": "graph",
        "subgraphId": "saddle-finance/saddle",
        "entity": "dailyVolumes",
        "orderDirection": "desc",
        "orderBy": "timestamp",
        "skip": 0,
        "first": 20,
        "filters": {}
      }
    },
    "transforms": {
      "uniswap.timestamp": "startOfDay",
      "saddle.timestamp": "startOfDay"
    },
    "join": {
      "saddle.timestamp": "uniswap.timestamp"
    },
    "group": {
      "key": "saddle.timestamp",
      "aggregations": {
        "saddle.volume": "sum"
      }
    }
  },
  "chart": {
    "xAxis": {
      "dataKey": "uniswap.timestamp",
      "format": "dateMMMdd"
    },
    "yAxis": {
      "format": "number"
    },
    "lines": [
      {
        "dataKey": "uniswap.dailyActiveUsers",
        "label": "Daily Active Users"
      },
      {
        "dataKey": "saddle.volume",
        "label": "Daily Volume"
      }
    ]
  }
}